	<?xml version="1.0" encoding="utf-8"?>
	<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui"
		onLoad="onLoadRibbon">
		<ribbon>
			<tabs>
				<tab id="ExplorerTab" label="JOA Tutorial">
					<group id="Group1" label="Happy Group">
						<button id="SmileButton" imageMso="HappyFace" size="large"
							label="Smile" onAction="onSmileButtonClicked" />
						<toggleButton id="JoaTaskPane" imageMso="HappyFace"
							size="large" label="Show/Hide" onAction="onJoaTaskPaneClicked"
							getPressed="onJoaTaskPaneGetPressed" />
						<button id="SmileButton2" getImage="onSmileButton2GetImage"
							size="large" label="Smile" onAction="onSmileButton2Clicked" />
					</group>
				</tab>
			</tabs>
		</ribbon>
	
		<contextMenus>
			<contextMenu idMso="ContextMenuMailItem">
				<menuSeparator id="MySeparator" />
				<button id="MyButton"  
					getLabel="getMenuButtonLabel"
					getImage="getMenuButtonImage"
					getVisible="getMenuButtonVisible"
					onAction="onMenuButtonClicked"
				/>
			</contextMenu>
		</contextMenus>
	</customUI>